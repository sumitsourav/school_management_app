<!-- app/views/schools/edit.html.erb -->
<h1>Edit School</h1>
<%= form_with(model: @school) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :phone %>
  <%= form.text_field :phone %>

  <%= form.submit %>
<% end %>
